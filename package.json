{
  "name": "angular-boilerplate",
  "version": "0.0.0",
  "packageManager": "yarn@1.22.22",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "serve": "ng serve --host 0.0.0.0",
    "build": "ng build",
    "build:serve": "npx http-server -p 8080 -c-1 dist/angular-boilerplate/browser",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "ng lint",
    "format-staged": "pretty-quick --staged",
    "write:env": "ngx-scripts env npm_package_version",
    "prettier": "prettier --write \"./src/**/*.{ts,js,html,scss}\"",
    "prettier:check": "prettier --list-different \"./src/**/*.{ts,js,html,scss}\"",
    "postinstall": "npm run prettier -s",
    "prepare": "husky"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^20.1.2",
    "@angular/common": "^20.1.2",
    "@angular/compiler": "^20.1.2",
    "@angular/core": "^20.1.2",
    "@angular/forms": "^20.1.2",
    "@angular/platform-browser": "^20.1.2",
    "@angular/platform-browser-dynamic": "^20.1.2",
    "@angular/router": "^20.1.2",
    "@angular/service-worker": "^20.1.2",
    "@ngneat/overview": "6.0.0",
    "@ngneat/until-destroy": "^10.0.0",
    "@ngx-translate/core": "16.0.4",
    "@ngx-translate/http-loader": "16.0.1",
    "@ngxpert/hot-toast": "5.0.1",
    "bootstrap": "5.3.7",
    "rxjs": "^7.8.2",
    "sanitize.css": "^13.0.0",
    "socket.io-client": "4.8.1",
    "tslib": "^2.8.1",
    "zone.js": "0.15.1"
  },
  "devDependencies": {
    "@angular/build": "^20.1.1",
    "@angular/cli": "^20.1.1",
    "@angular/compiler-cli": "^20.1.2",
    "@angular/language-service": "^20.1.2",
    "@angular/localize": "20.1.2",
    "@colsen1991/ngx-translate-extract-marker": "3.0.1",
    "@eslint/js": "^9.30.0",
    "@ngx-rocket/scripts": "^5.2.3",
    "@types/jasmine": "~5.1.0",
    "@types/node": "^22.5.5",
    "@vendure/ngx-translate-extract": "9.2.1",
    "angular-eslint": "20.1.1",
    "class-transformer": "^0.5.1",
    "eslint": "9.30.0",
    "eslint-config-prettier": "10.1.5",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-jsdoc": "51.3.1",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "eslint-plugin-prettier": "^5.5.1",
    "globals": "^15.9.0",
    "husky": "^9.1.6",
    "jasmine-core": "~5.2.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "postcss": "^8.5.6",
    "prettier": "3.6.2",
    "pretty-quick": "4.2.2",
    "reflect-metadata": "^0.2.1",
    "stylelint": "~14.11.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recommended-scss": "~7.0.0",
    "stylelint-config-standard": "~28.0.0",
    "ts-node": "^10.9.2",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.33.1"
  },
  "prettier": {
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.scss",
        "options": {
          "singleQuote": false
        }
      }
    ]
  }
}
